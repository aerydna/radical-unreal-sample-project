*.xcodeproj
*.xcworkspace
*.sln
*.suo
*.opensdf
*.sdf
*.VC.db
*.VC.opendb

# Precompiled Assets
SourceArt/**/*.png
SourceArt/**/*.tga

# Binary Files
RadicalLLSample/Binaries/*
RadicalLLSample/Plugins/*/Binaries/*

# Builds
RadicalLLSample/Build/*

# Whitelist PakBlacklist-<BuildConfiguration>.txt files
!RadicalLLSample/Build/*/
RadicalLLSample/Build/*/**
!RadicalLLSample/Build/*/PakBlacklist*.txt

# Don't ignore icon files in Build
!RadicalLLSample/Build/**/*.ico

# Built data for maps
RadicalLLSample/*_BuiltData.uasset

# Configuration files generated by the Editor
RadicalLLSample/Saved/*

# Compiled source files for the engine to use
RadicalLLSample/Intermediate/*
RadicalLLSample/Plugins/*/Intermediate/*

# Cache files for the editor to use
RadicalLLSample/DerivedDataCache/*
